{
  "name": "migraine-manager",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "npm run build:vue && npm run build:assets && npm run build:android",
    "build:vue": "vue-tsc -b && vite build",
    "build:assets": "npx capacitor-assets generate",
    "build:android": "npx cap sync && cd android && gradlew.bat assembleDebug",
    "send-to-android": "adb install -r android/app/build/outputs/apk/debug/app-debug.apk",
    "build-and-send": "npm run build && npm run send-to-android",
    "preview": "vite preview"
  },
  "dependencies": {
    "@capacitor/android": "^8.0.0",
    "@capacitor/assets": "^3.0.5",
    "@capacitor/core": "^8.0.0",
    "@capacitor/status-bar": "^8.0.0",
    "@quasar/extras": "^1.17.0",
    "@tailwindcss/vite": "^4.1.18",
    "pinia": "^3.0.4",
    "quasar": "^2.18.6",
    "tailwindcss": "^4.1.18",
    "vue": "^3.5.24",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@capacitor/cli": "^8.0.0",
    "@quasar/vite-plugin": "^1.10.0",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "prettier": "^3.7.4",
    "sass-embedded": "^1.97.1",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vue-tsc": "^3.1.4"
  }
}
